  "semanticHighlighting": true,
  "tokenColors":  [
    {
    "name": "Comments",
    "scope": [
      "comment",
      "punctuation.definition.comment"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#919aa2"
    }
    },
    {
    "name": "Comments: Preprocessor",
    "scope": "comment.block.preprocessor",
    "settings": {
      "fontStyle": "italic",
      "foreground": "#919aa2"
    }
    },
    {
    "name": "Comments: Documentation",
    "scope": [
      "comment.documentation",
      "comment.block.documentation"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#919aa2"
    }
    },
    {
    "name": "Invalid - Illegal",
    "scope": "invalid.illegal",
    "settings": {
      "foreground": "#BF3D34"
    }
    },
    {
    "name": "Operators",
    "scope": "keyword.operator",
    "settings": {
      "foreground": "#0000f3b0"
    }
    },
    {
    "name": "Keywords",
    "scope": [
      "keyword",
      "storage"
    ],
    "settings": {
      "foreground": "#0f8040",
      "fontStyle": "italic bold"
    }
    },
    {
    "name": "Types",
    "scope": [
      "storage.type",
      "support.type"
    ],
    "settings": {
      "foreground": "#8200f4",
      "fontStyle": "bold"
    }
    },
    {
    "name": "Language Constants",
    "scope": [
      "constant.language",
      "support.constant",
      "variable.language"
    ],
    "settings": {
      "foreground": "#004d25"
    }
    },
    {
    "name": "Variables",
    "scope": [
      "variable",
      "support.variable"
    ],
    "settings": {
      "foreground": "#2d2d2d",
    }
    },
    {
    "name": "Functions",
    "scope": [
      "entity.name.function",
      "support.function"
    ],
    "settings": {
      "fontStyle": "italic bold",
      "foreground": "#1f427c"
    }
    },
    {
    "name": "Classes",
    "scope": [
      "entity.name.type",
      "entity.other.inherited-class",
      "support.class"
    ],
    "settings": {
      "fontStyle": "bold",
      "foreground": "#9E0083"
    }
    },
    {
    "name": "Exceptions",
    "scope": "entity.name.exception",
    "settings": {
      "foreground": "#0586b1"
    }
    },
    {
    "name": "Sections",
    "scope": "entity.name.section",
    "settings": {
      "fontStyle": "bold"
    }
    },
    {
    "name": "Numbers, Characters",
    "scope": [
      "constant.numeric",
      "constant.character",
      "constant"
    ],
    "settings": {
      "foreground": "#0055ff",
      "fontStyle": "italic"
    }
    },
    {
    "name": "Strings",
    "scope": "string",
    "settings": {
      "foreground": "#c23f5f",
      // "fontStyle": "bold"
    }
    },
    {
    "name": "Strings: Escape Sequences",
    "scope": "constant.character.escape",
    "settings": {
      "foreground": "#469129",
      "fontStyle": "bold"
    }
    },
    {
    "name": "Strings: Regular Expressions",
    "scope": "string.regexp",
    "settings": {
      "foreground": "#4B83CD"
    }
    },
    {
    "name": "Strings: Symbols",
    "scope": "constant.other.symbol",
    "settings": {
      "foreground": "#2cc389"
    }
    },
    {
    "name": "Punctuation",
    "scope": "punctuation",
    "settings": {
      "foreground": "#444444",
      "fontStyle": "bold"
    }
    },
    {
    "name": "HTML: Doctype Declaration",
    "scope": [
      "meta.tag.sgml.doctype",
      "meta.tag.sgml.doctype string",
      "meta.tag.sgml.doctype entity.name.tag",
      "meta.tag.sgml punctuation.definition.tag.html"
    ],
    "settings": {
      "foreground": "#444444"
    }
    },
    {
    "name": "HTML: Tags",
    "scope": [
      "meta.tag",
      "punctuation.definition.tag.html",
      "punctuation.definition.tag.begin.html",
      "punctuation.definition.tag.end.html"
    ],
    "settings": {
      "foreground": "#16263B",
    }
    },
    {
    "name": "HTML: Tag Names",
    "scope": "entity.name.tag",
    "settings": {
      "foreground": "#4273B3"
    }
    },
    {
    "name": "HTML: Attribute Names",
    "scope": [
      "meta.tag entity.other.attribute-name",
      "entity.other.attribute-name.html"
    ],
    "settings": {
      "fontStyle": "italic",
      "foreground": "#621a8f"
    }
    },
    {
    "name": "HTML: Entities",
    "scope": [
      "constant.character.entity",
      "punctuation.definition.entity"
    ],
    "settings": {
      "foreground": "#E37500",
    }
    },
    {
    "name": "CSS: Selectors",
    "scope": [
      "meta.selector",
      "meta.selector entity",
      "meta.selector entity punctuation",
      "entity.name.tag.css"
    ],
    "settings": {
      "foreground": "#7A3E9D",
      "fontStyle": "bold",
    }
    },
    {
    "name": "CSS: Property Names",
    "scope": [
      "meta.property-name",
      "support.type.property-name"
    ],
    "settings": {
      "foreground": "#7A3E9D"
    }
    },
    {
    "name": "CSS: Property Values",
    "scope": [
      "meta.property-value",
      "meta.property-value constant.other",
      "support.constant.property-value"
    ],
    "settings": {
      "foreground": "#E37500"
    }
    },
    {
    "name": "CSS: Important Keyword",
    "scope": "keyword.other.important",
    "settings": {
      "foreground": "#AA3731",
      "fontStyle": "bold"
    }
    },
    {
    "name": "Markup: Changed",
    "scope": "markup.changed",
    "settings": {
      "foreground": "#444444"
    }
    },
    {
    "name": "Markup: Deletion",
    "scope": "markup.deleted",
    "settings": {
      "foreground": "#444444"
    }
    },
    {
    "name": "Markup: Emphasis",
    "scope": "markup.italic",
    "settings": {
      "fontStyle": "italic",
    }
    },
    {
    "name": "Markup: Error",
    "scope": "markup.error",
    "settings": {
      "foreground": "#660000"
    }
    },
    {
    "name": "Markup: Insertion",
    "scope": "markup.inserted",
    "settings": {
      "foreground": "#333333"
    }
    },
    {
    "name": "Markup: Link",
    "scope": "meta.link",
    "settings": {
      "foreground": "#4B83CD"
    }
    },
    {
    "name": "Markup: Output",
    "scope": [
      "markup.output",
      "markup.raw"
    ],
    "settings": {
      "foreground": "#555555"
    }
    },
    {
    "name": "Markup: Prompt",
    "scope": "markup.prompt",
    "settings": {
      "foreground": "#555555"
    }
    },
    {
    "name": "Markup: Heading",
    "scope": "markup.heading",
    "settings": {
      "foreground": "#AA3731"
    }
    },
    {
    "name": "Markup: Strong",
    "scope": "markup.bold",
    "settings": {
      "fontStyle": "bold"
    }
    },
    {
    "name": "Markup: Traceback",
    "scope": "markup.traceback",
    "settings": {
      "foreground": "#660000"
    }
    },
    {
    "name": "Markup: Underline",
    "scope": "markup.underline",
    "settings": {
      "fontStyle": "underline"
    }
    },
    {
    "name": "Markup Quote",
    "scope": "markup.quote",
    "settings": {
      "foreground": "#7A3E9D"
    }
    },
    {
    "name": "Markup Lists",
    "scope": "markup.list",
    "settings": {
      "foreground": "#4B83CD"
    }
    },
    {
    "name": "Markup Styling",
    "scope": [
      "markup.bold",
      "markup.italic"
    ],
    "settings": {
      "foreground": "#448C27"
    }
    },
    {
    "name": "Markup Inline",
    "scope": "markup.inline.raw",
    "settings": {
      "fontStyle": "",
      "foreground": "#AB6526"
    }
    },
    {
    "name": "Extra: Diff Range",
    "scope": [
      "meta.diff.range",
      "meta.diff.index",
      "meta.separator"
    ],
    "settings": {
      "foreground": "#444444"
    }
    },
    {
